{
    "buildCommand": "npm run build:web",
    "outputDirectory": "dist",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "crons": [
        {
            "path": "/api/keep-alive",
            "schedule": "0 0 */6 * *"
        }
    ]
}